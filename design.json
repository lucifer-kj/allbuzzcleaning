{
    "designSystem": {
      "name": "Crux - Professional Review System",
      "poweredBy": "Alpha Business Digital",
      "version": "1.0.0",
      "description": "A professional, minimal design system with enhanced accessibility and mobile-first approach"
    },
    "colorPalette": {
      "light": {
        "background": "hsl(0, 0%, 100%)",
        "foreground": "hsl(0, 0%, 0%)",
        "card": {
          "background": "hsl(0, 0%, 100%)",
          "foreground": "hsl(0, 0%, 0%)"
        },
        "popover": {
          "background": "hsl(0, 0%, 100%)",
          "foreground": "hsl(0, 0%, 0%)"
        },
        "primary": {
          "background": "hsl(0, 0%, 0%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "secondary": {
          "background": "hsl(0, 0%, 20%)",
          "foreground": "hsl(0, 0%, 0%)"
        },
        "muted": {
          "background": "hsl(0, 0%, 96%)",
          "foreground": "hsl(0, 0%, 45%)"
        },
        "accent": {
          "background": "hsl(0, 0%, 96%)",
          "foreground": "hsl(0, 0%, 0%)"
        },
        "destructive": {
          "background": "hsl(0, 84%, 60%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "success": {
          "background": "hsl(142, 76%, 36%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "border": "hsl(220, 13%, 91%)",
        "input": "hsl(220, 13%, 91%)",
        "ring": "hsl(0, 0%, 0%)",
        "sidebar": {
          "background": "hsl(0, 0%, 98%)",
          "foreground": "hsl(0, 0%, 0%)",
          "primary": "hsl(0, 0%, 0%)",
          "primaryForeground": "hsl(0, 0%, 100%)",
          "accent": "hsl(0, 0%, 96%)",
          "accentForeground": "hsl(0, 0%, 0%)",
          "border": "hsl(0, 0%, 96%)",
          "ring": "hsl(0, 0%, 0%)"
        },
        "star": {
          "empty": "hsl(0, 0%, 90%)",
          "filled": "hsl(45, 100%, 51%)",
          "hover": "hsl(45, 100%, 45%)"
        },
        "chart": {
          "1": "hsl(45, 100%, 51%)",
          "2": "hsl(220, 100%, 50%)",
          "3": "hsl(0, 0%, 72%)",
          "4": "hsl(0, 0%, 92%)",
          "5": "hsl(0, 0%, 56%)"
        }
      },
      "dark": {
        "background": "hsl(0, 0%, 0%)",
        "foreground": "hsl(0, 0%, 100%)",
        "card": {
          "background": "hsl(0, 0%, 14%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "popover": {
          "background": "hsl(0, 0%, 18%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "primary": {
          "background": "hsl(0, 0%, 100%)",
          "foreground": "hsl(0, 0%, 0%)"
        },
        "secondary": {
          "background": "hsl(0, 0%, 25%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "muted": {
          "background": "hsl(0, 0%, 23%)",
          "foreground": "hsl(0, 0%, 72%)"
        },
        "accent": {
          "background": "hsl(0, 0%, 32%)",
          "foreground": "hsl(0, 0%, 100%)"
        },
        "destructive": {
          "background": "hsl(0, 84%, 60%)",
          "foreground": "hsl(0, 0%, 0%)"
        },
        "border": "hsl(0, 0%, 26%)",
        "input": "hsl(0, 0%, 32%)",
        "ring": "hsl(0, 0%, 72%)",
        "sidebar": {
          "background": "hsl(240, 10%, 10%)",
          "foreground": "hsl(0, 0%, 100%)",
          "primary": "hsl(0, 0%, 100%)",
          "primaryForeground": "hsl(0, 0%, 0%)",
          "accent": "hsl(0, 0%, 32%)",
          "accentForeground": "hsl(0, 0%, 100%)",
          "border": "hsl(0, 0%, 32%)",
          "ring": "hsl(0, 0%, 72%)"
        },
        "chart": {
          "1": "hsl(45, 100%, 51%)",
          "2": "hsl(220, 100%, 50%)",
          "3": "hsl(0, 0%, 56%)",
          "4": "hsl(0, 0%, 44%)",
          "5": "hsl(0, 0%, 92%)"
        }
      }
    },
    "typography": {
      "fontFamilies": {
        "sans": ["Geist", "sans-serif"],
        "serif": ["Georgia", "serif"],
        "mono": ["Geist Mono", "monospace"]
      },
      "letterSpacing": {
        "normal": "0em",
        "tighter": "-0.05em",
        "tight": "-0.025em",
        "wide": "0.025em",
        "wider": "0.05em",
        "widest": "0.1em"
      },
      "scales": {
        "mobile": {
          "caption": "12px",
          "body": "14px",
          "text": "14px",
          "heading": "18px",
          "subheading": "16px"
        },
        "tablet": {
          "caption": "14px",
          "body": "16px", 
          "text": "16px",
          "heading": "20px",
          "subheading": "18px"
        },
        "desktop": {
          "caption": "14px",
          "body": "16px",
          "text": "18px",
          "heading": "24px",
          "subheading": "20px"
        }
      }
    },
    "spacing": {
      "system": {
        "xs": "0.25rem",
        "sm": "0.5rem", 
        "md": "1rem",
        "lg": "1.5rem",
        "xl": "2rem",
        "2xl": "3rem",
        "3xl": "4rem"
      },
      "semantic": {
        "section": "32px",
        "content": "24px",
        "form": "24px", 
        "card": "16px"
      },
      "mobile": {
        "container": "12px",
        "gap": "8px",
        "gapLg": "12px",
        "spaceY": "12px",
        "spaceX": "8px"
      }
    },
    "borderRadius": {
      "sm": "0.375rem",
      "md": "0.5rem", 
      "lg": "0.75rem",
      "xl": "1rem",
      "2xl": "1.5rem"
    },
    "shadows": {
      "light": {
        "sm": "0px 1px 2px 0px hsl(0 0% 0% / 0.08), 0px 1px 3px -1px hsl(0 0% 0% / 0.08)",
        "default": "0px 1px 3px 0px hsl(0 0% 0% / 0.12), 0px 1px 2px -1px hsl(0 0% 0% / 0.12)",
        "md": "0px 4px 6px -1px hsl(0 0% 0% / 0.12), 0px 2px 4px -1px hsl(0 0% 0% / 0.12)",
        "lg": "0px 10px 15px -3px hsl(0 0% 0% / 0.12), 0px 4px 6px -2px hsl(0 0% 0% / 0.12)",
        "xl": "0px 20px 25px -5px hsl(0 0% 0% / 0.12), 0px 10px 10px -5px hsl(0 0% 0% / 0.12)",
        "2xl": "0px 1px 2px 0px hsl(0 0% 0% / 0.45)"
      },
      "dark": {
        "sm": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 1px 2px -1px hsl(0 0% 0% / 0.18)",
        "default": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 1px 2px -1px hsl(0 0% 0% / 0.18)",
        "md": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 2px 4px -1px hsl(0 0% 0% / 0.18)",
        "lg": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 4px 6px -1px hsl(0 0% 0% / 0.18)",
        "xl": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 8px 10px -1px hsl(0 0% 0% / 0.18)",
        "2xl": "0px 1px 2px 0px hsl(0 0% 0% / 0.45)"
      }
    },
    "animations": {
      "durations": {
        "fast": "150ms",
        "normal": "200ms", 
        "slow": "300ms"
      },
      "easings": {
        "out": "cubic-bezier(0.4, 0, 0.2, 1)",
        "in": "cubic-bezier(0.4, 0, 1, 1)",
        "inOut": "cubic-bezier(0.4, 0, 0.2, 1)"
      },
      "keyframes": {
        "fadeIn": {
          "from": {
            "opacity": "0",
            "transform": "translateY(10px)"
          },
          "to": {
            "opacity": "1",
            "transform": "translateY(0)"
          }
        },
        "slideUp": {
          "from": {
            "opacity": "0",
            "transform": "translateY(20px)"
          },
          "to": {
            "opacity": "1", 
            "transform": "translateY(0)"
          }
        },
        "scaleIn": {
          "from": {
            "opacity": "0",
            "transform": "scale(0.95)"
          },
          "to": {
            "opacity": "1",
            "transform": "scale(1)"
          }
        },
        "bounceIn": {
          "0%": {
            "opacity": "0",
            "transform": "scale(0.3)"
          },
          "50%": {
            "opacity": "1",
            "transform": "scale(1.05)"
          },
          "70%": {
            "transform": "scale(0.9)"
          },
          "100%": {
            "opacity": "1",
            "transform": "scale(1)"
          }
        },
        "slideInLeft": {
          "from": {
            "opacity": "0",
            "transform": "translateX(-30px)"
          },
          "to": {
            "opacity": "1",
            "transform": "translateX(0)"
          }
        },
        "slideInRight": {
          "from": {
            "opacity": "0",
            "transform": "translateX(30px)"
          },
          "to": {
            "opacity": "1",
            "transform": "translateX(0)"
          }
        }
      }
    },
    "components": {
      "button": {
        "base": {
          "borderRadius": "0.75rem",
          "fontWeight": "500",
          "transition": "all 200ms cubic-bezier(0.4, 0, 0.2, 1)",
          "focus": {
            "outline": "2px solid var(--ring)",
            "outlineOffset": "2px"
          }
        },
        "sizes": {
          "sm": {
            "height": "32px",
            "padding": "12px",
            "fontSize": "12px"
          },
          "default": {
            "height": "40px",
            "padding": "16px",
            "fontSize": "14px"
          },
          "lg": {
            "height": "48px",
            "padding": "24px",
            "fontSize": "16px"
          }
        },
        "mobile": {
          "minHeight": "44px",
          "minWidth": "44px"
        }
      },
      "card": {
        "base": {
          "backgroundColor": "var(--card)",
          "border": "1px solid var(--border)",
          "borderRadius": "0.75rem",
          "padding": "24px",
          "boxShadow": "var(--shadow-sm)",
          "transition": "all 200ms cubic-bezier(0.4, 0, 0.2, 1)"
        },
        "hover": {
          "boxShadow": "var(--shadow-md)"
        },
        "mobile": {
          "borderRadius": "0.5rem",
          "padding": "12px"
        }
      },
      "form": {
        "container": {
          "backgroundColor": "var(--card)",
          "border": "1px solid var(--border)",
          "borderRadius": "0.75rem",
          "padding": "32px",
          "boxShadow": "var(--shadow-lg)",
          "maxWidth": "448px",
          "margin": "0 auto",
          "backdropFilter": "blur(4px)"
        },
        "field": {
          "marginBottom": "16px"
        },
        "label": {
          "fontSize": "14px",
          "fontWeight": "600",
          "color": "var(--foreground)",
          "letterSpacing": "0.025em",
          "marginBottom": "4px"
        },
        "input": {
          "width": "100%",
          "padding": "12px 16px",
          "border": "1px solid var(--border)",
          "borderRadius": "0.5rem",
          "backgroundColor": "var(--input)",
          "color": "var(--foreground)",
          "transition": "all 200ms cubic-bezier(0.4, 0, 0.2, 1)",
          "fontSize": "14px",
          "focus": {
            "border": "1px solid var(--ring)",
            "boxShadow": "var(--shadow-sm)"
          }
        },
        "mobile": {
          "container": {
            "padding": "20px"
          },
          "input": {
            "height": "40px",
            "padding": "12px",
            "fontSize": "16px"
          }
        }
      },
      "starRating": {
        "container": {
          "display": "flex",
          "alignItems": "center",
          "gap": "4px"
        },
        "button": {
          "backgroundColor": "transparent",
          "border": "none",
          "padding": "0",
          "cursor": "pointer",
          "transition": "all 200ms cubic-bezier(0.4, 0, 0.2, 1)",
          "borderRadius": "4px",
          "focus": {
            "outline": "2px solid var(--ring)",
            "outlineOffset": "2px"
          }
        },
        "star": {
          "transition": "all 200ms cubic-bezier(0.4, 0, 0.2, 1)"
        },
        "states": {
          "active": {
            "color": "var(--star-filled)"
          },
          "hoverActive": {
            "color": "var(--star-hover)"
          },
          "focused": {
            "color": "var(--star-filled)",
            "transform": "scale(1.05)"
          }
        }
      }
    },
    "accessibility": {
      "focusRing": {
        "width": "2px",
        "style": "solid",
        "color": "var(--ring)",
        "offset": "2px"
      },
      "screenReaderOnly": {
        "position": "absolute",
        "width": "1px",
        "height": "1px",
        "padding": "0",
        "margin": "-1px",
        "overflow": "hidden",
        "clip": "rect(0, 0, 0, 0)",
        "whiteSpace": "nowrap",
        "border": "0"
      },
      "skipLink": {
        "position": "absolute",
        "top": "-40px",
        "left": "24px",
        "zIndex": "50",
        "backgroundColor": "var(--primary)",
        "color": "var(--primary-foreground)",
        "padding": "8px 16px",
        "borderRadius": "6px",
        "focus": {
          "top": "16px"
        }
      },
      "minTouchTarget": {
        "minHeight": "44px",
        "minWidth": "44px"
      }
    },
    "responsive": {
      "breakpoints": {
        "sm": "640px",
        "md": "768px", 
        "lg": "1024px",
        "xl": "1280px",
        "2xl": "1536px"
      },
      "container": {
        "mobile": "12px 16px",
        "tablet": "24px 32px", 
        "desktop": "32px 48px"
      }
    },
    "utilityClasses": {
      "spacing": [
        "space-section",
        "space-content", 
        "space-form",
        "space-card",
        "mobile-space-y",
        "mobile-space-x",
        "mobile-gap",
        "mobile-gap-lg"
      ],
      "animations": [
        "fade-in",
        "slide-up",
        "scale-in", 
        "bounce-in",
        "slide-in-left",
        "slide-in-right"
      ],
      "interactions": [
        "interactive-hover",
        "interactive-lift",
        "interactive-glow",
        "hover-subtle",
        "hover-elevate",
        "hover-emphasize"
      ],
      "mobile": [
        "mobile-container",
        "mobile-text",
        "mobile-heading",
        "mobile-subheading", 
        "mobile-body",
        "mobile-caption",
        "mobile-touch-target",
        "mobile-button",
        "mobile-button-sm",
        "mobile-button-lg",
        "mobile-table",
        "mobile-form-field",
        "mobile-form-label",
        "mobile-form-input",
        "mobile-card"
      ]
    },
    "implementation": {
      "cssVariables": {
        "required": true,
        "prefix": "--",
        "fallbacks": true
      },
      "frameworks": {
        "tailwindcss": {
          "compatible": true,
          "configPath": "tailwind.config.js",
          "layerOrder": ["base", "components", "utilities"]
        },
        "styledComponents": {
          "compatible": true,
          "themeStructure": "nested"
        },
        "emotion": {
          "compatible": true,
          "themeStructure": "nested"
        },
        "chakraUI": {
          "compatible": true,
          "mappingRequired": true
        },
        "materialUI": {
          "compatible": true,
          "customThemeRequired": true
        }
      },
      "dependencies": {
        "fonts": ["Geist", "Geist Mono"],
        "icons": ["lucide-react (recommended)"],
        "cssFeatures": ["CSS Custom Properties", "CSS Grid", "Flexbox"]
      }
    }
  }